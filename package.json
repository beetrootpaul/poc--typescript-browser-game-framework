{
  "name": "poc--typescript-web-game-framework",
  "version": "0.0.0",
  "author": "Beetroot Paul",
  "license": "MIT",
  "private": true,
  "description": "A PoC of a web game framework written in TypeScript.",
  "homepage": "https://github.com/beetrootpaul/poc--typescript-web-game-framework#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beetrootpaul/poc--typescript-web-game-framework.git"
  },
  "bugs": {
    "url": "https://github.com/beetrootpaul/poc--typescript-web-game-framework/issues"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "start": "npm run typecheck:watch",
    "typecheck": "tsc",
    "typecheck:watch": "tsc --watch",
    "format": "prettier --write src/",
    "cleanup": "rimraf ./**/*.d.ts ./tsconfig.tsbuildinfo"
  },
  "lint-staged": {
    "src/**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.1",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "ts-jest": "29.1.1",
    "typescript": "5.0.4"
  }
}
